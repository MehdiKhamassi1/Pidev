{% extends 'baseback.html.twig' %}

{% block title %}docteur  index{% endblock %}
 {% block body %}
 {% endblock %}
{% block content %}
    <h1>Admin</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ admin.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ admin.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ admin.roles ? admin.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ admin.password }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ admin.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ admin.prenom }}</td>
            </tr>
            <tr>
                <th>Numtel</th>
                <td>{{ admin.numtel }}</td>
            </tr>
            <tr>
                <th>Birth</th>
                <td>{{ admin.birth ? admin.birth|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Profile_image</th>
                <td>{{ admin.profileImage }}</td>
            </tr>
            <tr>
                <th>Gender</th>
                <td>{{ admin.gender }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_index') }}">back to list</a>

    <a href="{{ path('app_admin_edit', {'id': admin.id}) }}">edit</a>

    {{ include('admin/_delete_form.html.twig') }}
